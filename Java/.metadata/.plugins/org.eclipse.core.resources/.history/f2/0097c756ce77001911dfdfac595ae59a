package acao;

public class Acao {
	
	//Validar repeticao
		public boolean validar(IMC c) {
			
			for(int i = 0;i<Dado.arrayDados.size();i++) {
				if( (Dado.arrayDados.get(i).getNome().contentEquals(c.getNome())) && (Dado.arrayDados.get(i).getPlataforma().contentEquals(c.getPlataforma())) ) {
					return true;
				}
			}
			
			return false;
		}

}
