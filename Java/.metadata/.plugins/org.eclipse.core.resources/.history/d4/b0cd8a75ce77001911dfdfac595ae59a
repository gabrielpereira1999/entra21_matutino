package acao;

import dado.Pessoas;

public class Acao {
	
	//Validar repeticao
		public boolean validar(IMC c) {
			
			for(int i = 0;i<Pessoas.arrayIMC.size();i++) {
				if( (Pessoas.arrayIMC.get(i).getNome().contentEquals(c.getNome())) ) {
					return false;
				}
			}
			
			return true;
		}

}
