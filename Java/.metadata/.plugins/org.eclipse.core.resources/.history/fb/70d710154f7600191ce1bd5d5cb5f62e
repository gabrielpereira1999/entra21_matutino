package beans;

public class Colaborador {
	
	private String nome,cargo;
	private Double salBruto,salLiquido,imposto;
	private Boolean vale,saude,clube;
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	public String getCargo() {
		return cargo;
	}
	public void setCargo(String cargo) {
		this.cargo = cargo;
	}
	public Double getSalBruto() {
		return salBruto;
	}
	public void setSalBruto(Double salBruto) {
		this.salBruto = salBruto;
	}
	public Double getSalLiquido() {
		return salLiquido;
	}
	public void setSalLiquido(Double salLiquido) {
		this.salLiquido = salLiquido;
	}
	public Double getImposto() {
		return imposto;
	}
	public void setImposto(Double imposto) {
		this.imposto = imposto;
	}
	public Boolean getVale() {
		return vale;
	}
	public void setVale(Boolean vale) {
		this.vale = vale;
	}
	public Boolean getSaude() {
		return saude;
	}
	public void setSaude(Boolean saude) {
		this.saude = saude;
	}
	public Boolean getClube() {
		return clube;
	}
	public void setClube(Boolean clube) {
		this.clube = clube;
	}
	

}
